function changeDeadline(DeadlineHTML) {
  const deadline = document.getElementById('survey-deadline');
  deadline.innerHTML = DeadlineHTML;
}

if (document.querySelector("#survey-deadline")) {
  changeDeadline('<%= j render "surveys/deadline", survey: @survey %>');
}
